<launch>

    <rosparam command="load" file="$(find ros_cellphonerobot)/rosparam/profile.yaml"/>

    <node pkg="ros_cellphonerobot" name="ultrasonic" type="ultrasonic.py">
    </node>
    
    <node pkg="ros_cellphonerobot" name="sensor_hub" type="sensor_hub.py"/>
	<node pkg="ros_cellphonerobot" name="exe_server" type="exe_server.py" output="screen" />
    <node pkg="ros_cellphonerobot" name="execution" type="execution.py" output="screen"/>
    <node pkg="ros_cellphonerobot" name="tf_broadcaster" type="tf_broadcaster.py" output="screen"/>
    <node pkg="ros_cellphonerobot" type="pid_velocity.py" name="lpid_velocity">
      <remap from="wheel" to="lwheel"/>
      <remap from="motor_cmd" to="lmotor_cmd"/>
      <remap from="wheel_vtarget" to="lwheel_vtarget"/>
      <remap from="wheel_vel" to="lwheel_vel"/>
      <rosparam param="Kp">0</rosparam>
      <rosparam param="Ki">0</rosparam>
      <rosparam param="Kd">0</rosparam>
      <rosparam param="out_min">0</rosparam>
      <rosparam param="out_max">100</rosparam>
      <rosparam param="rate">30</rosparam>
      <rosparam param="timeout_ticks">4</rosparam>
      <rosparam param="rolling_pts">2</rosparam>
  </node>
  <node pkg="ros_cellphonerobot" type="pid_velocity.py" name="rpid_velocity">
      <remap from="wheel" to="rwheel"/>
      <remap from="motor_cmd" to="rmotor_cmd"/>
      <remap from="wheel_vtarget" to="rwheel_vtarget"/>
      <remap from="wheel_vel" to="rwheel_vel"/>
      <rosparam param="Kp">0</rosparam>
      <rosparam param="Ki">0</rosparam>
      <rosparam param="Kd">0</rosparam>
      <rosparam param="out_min">0</rosparam>
      <rosparam param="out_max">0</rosparam>
      <rosparam param="rate">30</rosparam>
      <rosparam param="timeout_ticks">4</rosparam>
      <rosparam param="rolling_pts">2</rosparam>
  </node>
  <node pkg="ros_cellphonerobot" type="twist_to_motors.py" name="twist_to_motors" output="screen">
    <rosparam param="base_width">0.185</rosparam>
  </node>
  
  <!--
  <param name="robot_description" textfile="$(find my_robot)/my_robot.urdf" />
  <node pkg="rviz" type="rviz" name="rviz" output="screen"/>
  -->

  <node pkg="ros_cellphonerobot" type="diff_tf.py" name="diff_tf">
     <rosparam param="rate">8.0</rosparam>
     <rosparam param="base_width">0.185</rosparam>
  </node>
  <node pkg="ros_cellphonerobot" type="virtual_joystick.py" name="virtual_joystick" output="screen"/>

    <!--
    Uncomment this line to start image classification node.
    <node pkg="ros_cellphonerobot" name="image_classify" type="image_classify.py" output="screen"/>
    -->
    <!--
     <node pkg="ros_cellphonerobot" name="key_publisher" type="key_publisher.py"/>
    --> 

</launch>
